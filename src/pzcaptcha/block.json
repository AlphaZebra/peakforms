{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "peakforms/pzcaptcha",
	"version": "0.1.0",
	"title": "Peakforms Captcha",
	"category": "peakzebra",
	"icon": "shield",
	"description": "Adds a reCAPTCHA challenge to your form",
	"example": {},
	"attributes": {
		"siteKey": {
			"type": "string",
			"default": ""
		},
		"size": {
			"type": "string",
			"default": "normal"
		},
		"theme": {
			"type": "string",
			"default": "light"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "peakforms",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
